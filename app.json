{
  "expo": {
    "name": "CraveCrust",
    "slug": "crave-crust",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon@3x.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon@2x.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sahanudara.cravecrust"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "378ccc53-bd6c-462c-9217-56c4f7d1de65"
      }
    }
  }
}
